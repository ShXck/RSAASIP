module ALU_TB();

	parameter ARQ = 16;
	
	logic [ARQ - 1:0] d1, d2, d3, result;
	logic z;
	logic [1:0] contrl;
	
	ALU uut(d1, d2, d3, contrl, z);
	
	initial begin
		#0 d1 = 16'd10; d2 = 16'd15; d3 = 16'd15;
		#100 ctrl = 2'b0;
		#100 ctrl = 2'b1;
		#100 ctrl = 2'b0;
		#100 ctrl = 2'b0;
	end
	

endmodule
